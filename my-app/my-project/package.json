    "@theatre/core": "^0.7.0",
    "@theatre/studio": "^0.7.0",
    "@threlte/core": "^6.1.0",
    "@threlte/extras": "^7.3.0",
    "@threlte/flex": "^0.0.9",
    "@threlte/rapier": "^1.1.2",
    "@threlte/theatre": "^2.1.3",
    "@threlte/xr": "^0.0.11",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.28.0",
